import java.util.Scanner;
class Main
{
    	public static void main(String args[])
        {
            Scanner don = new Scanner(System.in);
          	int first_number = don.nextInt();
          	int second_number = don.nextInt();
          	System.out.println(first_number + second_number);
          	System.out.println(first_number - second_number);
        }
}
