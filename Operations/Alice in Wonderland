import java.util.Scanner;
class Main
{
	public static void main(String args[])
	{
		Scanner get = new Scanner(System.in);
      	int a = get.nextInt();
      	int sum = 0;
        	for(int i = 0; i < 1;i++)
            {
            	int b = a % 10;
              	a = a / 10;
              	sum = b + a;
           
            }
          System.out.print("Alice must go in path-"+sum);
        
	}
}
