import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner mark = new Scanner(System.in);
      	float stud1 = mark.nextFloat();
      	float stud2 = mark.nextFloat();
      	float stud3 = mark.nextFloat();
      	float stud4 = mark.nextFloat();
      	float stud5 = mark.nextFloat();
      	float total = stud1 + stud2 + stud3 + stud4 + stud5;
      	float avg = total/5;
      	System.out.printf("%.2f",avg);
      	
    }
}
