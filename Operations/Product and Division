import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner don = new Scanner(System.in);
          	int a = don.nextInt();
          	int b = don.nextInt();
          	System.out.println(a*b);
      System.out.println(a/b);
         System.out.println(a%b);
                
    }
            
}
