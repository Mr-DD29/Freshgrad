import java.util.Scanner;
class Main
{
public static void main(String args[])
{
	Scanner get = new Scanner(System.in);
  	int student = get.nextInt();
  	int team = get.nextInt();
  	int div = student / team;
  	int mod = student % team;
 	System.out.print("The number of students in each team is "+div+" and left out is "+mod);
}
}
