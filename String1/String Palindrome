import java.util.Scanner;
class Main
{
	public static void main(String args[])
	{
		Scanner vicky = new Scanner(System.in);
      	String a = vicky.next();
      	String b = "";
      	for(int i = a.length() - 1; i >= 0; i--)
        {
        	b = b + a.charAt(i);
        }
      	if(a.equals(b))
          System.out.print("Palindrome");
      	else
          System.out.print("Not a Palindrome");
	}
}
