import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner get = new Scanner(System.in);
      	int a = get.nextInt();
      	int b = get.nextInt();
      	int c = get.nextInt();
      	int student = get.nextInt();
		int count = 0;
      	if(student <= a)
        {
        	count ++;
          	if(student <= b)
            	count ++;
          	if(student <= c)
              	count ++;
        }
      	else
        {
        	if(student <= b)
            	count ++;
          	else if(student <= c)
              	count ++;
        }
      System.out.print(count);
    }
}
