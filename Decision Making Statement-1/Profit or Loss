import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner get = new Scanner(System.in);
      	float a = get.nextFloat();
      	float b = get.nextFloat();
      	float result = b * 12;
      	float c = result - a;
      	if(c > 0)
          	System.out.printf("Profit : Rs.%.2f"+c);
      	else if(c < 0)
          	System.out.printf("Loss : Rs.%.2f"+c);
      	else
          	System.out.print("No profit nor loss");
      	
    }
}
