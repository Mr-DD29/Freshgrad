import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner get = new Scanner(System.in);
      	int a = get.nextInt();
      	int b = get.nextInt();
      	int c = get.nextInt();
      	int d = get.nextInt();
      	if((a - d) <= 10)
          System.out.print("L1");
      	else if((b - d) <= 10)
          System.out.print("L2");
      	else if((c - d) <= 10)
          System.out.print("L3");
    }
}
