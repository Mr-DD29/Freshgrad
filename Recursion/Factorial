import java.util.Scanner;
import java.lang.Math;
class Main
{
  	public static int fact(int a)
    {
    	if(a == 1)
          return 1;
      	else
          return a * fact(a - 1);
    }
    public static void main(String args[])
    {
        Scanner vicky = new Scanner(System.in);
      	int n = vicky.nextInt();
      	System.out.print("The factorial of "+n+" is "+fact(n));
    }
}
